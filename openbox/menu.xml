<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/  file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" title="Menu" label="Openbox 3">
<!--	<separator label="Main Menu"/>  -->
		<item label="Terminal Emulator">
			<action name="Execute">
				<command>termite</command>
			</action>
		</item>
		<item label="Web Browser">
			<action name="Execute">
				<command>qutebrowser</command>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<command>pcmanfm</command>
			</action>
		</item>
		<item label="Text Editor">
			<action name="Execute">
				<command>emacs</command>
			</action>
		</item>
		<separator/>
		<menu execute="obmenu-generator" id="app-menu" label="Applications"/>
		<menu id="root-menu-8397" label="Apps As Root">
			<item label="Terminal (termite)">
				<action name="Execute">
					<execute>apps_as_root.sh termite</execute>
				</action>
			</item>
			<item label="File Manager (pcmanfm)">
				<action name="Execute">
					<execute>apps_as_root.sh 'dbus-run-session pcmanfm'</execute>
				</action>
			</item>
			<item label="Text Editor (geany)">
				<action name="Execute">
					<execute>apps_as_root.sh emacs</execute>
				</action>
			</item>
			<item label="Text Editor Alt (leafpad)">
				<action name="Execute">
					<execute>apps_as_root.sh leafpad</execute>
				</action>
			</item>
			<item label="Text Editor CLI (vim)">
				<action name="Execute">
					<execute>apps_as_root.sh 'termite -e nvim'</execute>
				</action>
			</item>
		</menu>

		<menu id="root-menu-srct" label="Shortcuts">
			<item label="Color Chooser">
				<action name="Execute">
					<execute>color_picker</execute>
				</action>
			</item>
			<item label="Stick Notes">
				<action name="Execute">
					<execute>tasks</execute>
				</action>
			</item>
			<item label="Music Player">
				<action name="Execute">
					<execute>deadbeef</execute>
				</action>
			</item>
			<item label="Visualizer">
				<action name="Execute">
					<execute>termite -e 'cava'</execute>
				</action>
			</item>
		</menu>

		<menu id="root-menu-8399" label="Quick Links">
			<item label="DuckDuckGo">
				<action name="Execute">
					<execute>qutebrowser https://start.duckduckgo.com/</execute>
				</action>
			</item>
			<item label="Google">
				<action name="Execute">
					<execute>qutebrowser https://www.google.com</execute>
				</action>
			</item>
			<item label="Facebook">
				<action name="Execute">
					<execute>qutebrowser https://www.facebook.com</execute>
				</action>
			</item>
			<item label="Twitter">
				<action name="Execute">
					<execute>qutebrowser https://www.twitter.com</execute>
				</action>
			</item>
			<item label="GitHub">
				<action name="Execute">
					<execute>qutebrowser https://www.github.com</execute>
				</action>
			</item>
			<item label="Reddit">
				<action name="Execute">
					<execute>qutebrowser https://www.reddit.com</execute>
				</action>
			</item>
			<item label="Gmail">
				<action name="Execute">
					<execute>qutebrowser https://mail.google.com</execute>
				</action>
			</item>
			<item label="Youtube">
				<action name="Execute">
					<execute>qutebrowser https://www.youtube.com</execute>
				</action>
			</item>
			<separator/>
			<item label="Private">
				<action name="Execute">
					<execute>qutebrowser -p</execute>
				</action>
			</item>
		</menu>
		<menu id="root-menu-8365" label="Screenshot">
			<item label="Screenshot Now">
				<action name="Execute">
					<execute>shotnow</execute>
				</action>
			</item>
			<item label="Screenshot In 5s">
				<action name="Execute">
					<execute>shotin5</execute>
				</action>
			</item>
			<item label="Screenshot In 10s">
				<action name="Execute">
					<execute>shotin10</execute>
				</action>
			</item>
			<item label="Screenshot Area">
				<action name="Execute">
					<execute>shotArea</execute>
				</action>
			</item>
			<item label="Screenshot Window">
				<action name="Execute">
					<execute>shotWindow</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="obConfig" label="Openbox">
			<item label="Settings">
				<action name="Execute">
					<command>obconf</command>
				</action>
			</item>
			<separator/>
			<item label="Edit menu.xml">
				<action name="Execute">
					<command>exo-open ~/.config/openbox/menu.xml</command>
				</action>
			</item>
			<item label="Edit rc.xml">
				<action name="Execute">
					<command>exo-open ~/.config/openbox/rc.xml</command>
				</action>
			</item>
			<item label="Edit autostart">
				<action name="Execute">
					<command>exo-open ~/.config/openbox/autostart</command>
				</action>
			</item>
			<separator/>
			<item label="Openbox Reconfigure">
				<action name="Reconfigure"/>
			</item>
			<item label="Openbox Restart">
				<action name="Restart"/>
			</item>
		</menu>
		<menu execute="ob-places-pipemenu --recent ~/" id="pipe-placesmenu" label="Places"/>
		<menu execute="ob-kb-pipemenu" id="keybinds" label="Keybinds"/>
		<separator/>
		<menu id="obExit" label="System">
				<item label="Logout">
					<action name="Execute">
						<command>ob_powermenu --logout</command>
					</action>
				</item>
				<item label="Suspend">
					<action name="Execute">
						<command>ob_powermenu --suspent</command>
					</action>
				</item>
				<item label="Reboot">
					<action name="Execute">
						<command>ob_powermenu --reboot</command>
					</action>
				</item>
				<item label="Shutdown">
					<action name="Execute">
						<command>ob_powermenu --shutdown</command>
					</action>
				</item>
		</menu>
	</menu>
</openbox_menu>
